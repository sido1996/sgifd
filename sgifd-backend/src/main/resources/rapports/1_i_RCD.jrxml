	<queryString>
		<![CDATA[SELECT a.libelle,
	SUM( t.montant_ressource_engager)
FROM t_ressource_interieure_annuelle t,
	t_annee a
WHERE
	 t.annee_id = a.id

	 AND a.libelle BETWEEN $P{annee_Debut} AND  $P{annee_Fin}
GROUP BY a.libelle
ORDER BY a.libelle ASC]]>
	</queryString>
