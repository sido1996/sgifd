	<queryString>
		<![CDATA[SELECT sRequete1.ANNEE, sRequete1.GROUPE_DONATEUR, MONTANT
FROM
( SELECT  tc.libelle AS GROUPE_DONATEUR, a.libelle AS ANNEE
 FROM t_categorieptf tc, t_annee a
 WHERE  tc.status = false
AND a.status = false
) sRequete1 LEFT JOIN
(
SELECT  a.libelle AS ANNEE, tc.libelle AS GROUPE_DONATEUR, SUM(t.montant_ressource_decaisser_fcfa) AS MONTANT
FROM t_ptfbailleur_frs p, t_annee, t_ressource_exterieure tr, t_annee a, t_ressource_exterieure_annuelle t,
t_categorieptf tc
WHERE t.annee_id = a.id
AND tr.ptfbailleur_frs_id = p.id
AND t.ressource_exterieure_id = tr.id
AND tc.id = p.categorieptf_id
AND tc.status = false
AND a.status = false
GROUP BY a.libelle, tc.libelle
ORDER BY a.libelle ASC
	) sRequete2 ON sRequete1.GROUPE_DONATEUR = sRequete2.GROUPE_DONATEUR AND sRequete1.ANNEE = sRequete2.ANNEE
	WHERE sRequete1.ANNEE BETWEEN $P{annee_Debut} AND  $P{annee_Fin}
	ORDER BY ANNEE ASC]]>
	</queryString>
