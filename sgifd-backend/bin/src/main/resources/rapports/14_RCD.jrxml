	<queryString>
		<![CDATA[SELECT sRequete1.ANNEE, sRequete1.TYPE_COOPERATION, MONTANT
FROM
( SELECT t_annee.libelle ANNEE, t_type_cooperation.libelle TYPE_COOPERATION
	FROM t_type_cooperation, t_annee
WHERE t_type_cooperation.status = false
AND t_annee.status = false
	) sRequete1 LEFT JOIN
(
SELECT a.libelle ANNEE, t.libelle TYPE_COOPERATION, SUM(c.montant) MONTANT
FROM t_cooperation_decentralisee c, t_type_cooperation t, t_annee a
WHERE  c.exercice_id = a.id
AND   c.type_cooperation_id = t.id
GROUP BY a.libelle, t.libelle
		) sRequete2 ON sRequete1.ANNEE = sRequete2.ANNEE AND sRequete1.TYPE_COOPERATION = sRequete2.TYPE_COOPERATION
		WHERE  sRequete1.ANNEE BETWEEN $P{annee_Debut} AND  $P{annee_Fin}]]>
	</queryString>
